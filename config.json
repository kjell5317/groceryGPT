{
  "name": "groceryGPT",
  "version": "0.2",
  "slug": "grocerygpt",
  "url": "https://github.com/kjell5317/groceryGPT",
  "description": "The smart shopping list.",
  "arch": ["armhf", "armv7", "aarch64", "amd64", "i386"],
  "startup": "application",
  "boot": "auto",
  "options": {
    "api_key": null,
    "categories": [
      "Fruits",
      "Vegetables",
      "Dairy",
      "Meat",
      "Bakery",
      "Beverages"
    ]
  },
  "schema": {
    "api_key": "password",
    "categories": ["match(^\\w*$)"]
  },
  "init": false
}
